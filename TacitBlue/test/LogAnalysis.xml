<?xml version="1.0" encoding="UTF-8"?>
<testCase>
	<title>SampleTest02</title>
	<description>This is just a simple test case for IC automation log analysis. </description>
	<operation step="Check BPMConfig Log">
		<name>CheckLog</name>
		<points>5</points>
		<action>check</action>
		<type>BPMConfigLog</type>
	</operation>
	<operation step="Check All BPM Server Log">
		<name>CheckLog</name>
		<points>5</points>
		<action>check</action>
		<type>BPMLog</type>
	</operation>
	<operation step="Check BPM Server Log">
		<name>CheckLog</name>
		<points>5</points>
		<action>check</action>
		<type>BPMLog</type>
		<option>BPMNodeServerLog</option>
	</operation>
	<operation step="Check BPM Server FFDC">
		<name>CheckLog</name>
		<points>5</points>
		<action>check</action>
		<type>BPMLog</type>
		<option>BPMNodeFFDCLog</option>
	</operation>
	<operation step="Start BPM Server">
		<name>BPMServer</name>
		<points>1</points>
		<action>start</action>
	</operation>
</testCase>